{
  "scripts": {
    "build": "webpack --mode production",
    "wds": "node webpack-dev-server.js",
    "client": "webpack-dev-server --mode development",
    "server": "nodemon --config ./Server/nodemon.json --exec babel-node ./Server/server.js",
    "debug": "nodemon --config ./Server/nodemon.json --inspect ./Server/server.js --require=babel-register",
    "forever-server": "forever -c 'nodemon --exitcrash --exec babel-node' Server/server.js",
    "admin": "yarn run server & yarn run client",
    "dev": "concurrently \"yarn run server\" \"yarn run client\""
  },
